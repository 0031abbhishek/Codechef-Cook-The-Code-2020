#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        long long int a,b,c;
        cin>>a>>b>>c;
        if(c%a==b){
            cout<<c<<endl;
        }
        else if(c%a>b){
            cout<<c-(c%a-b)<<endl;
        }
        else{
            cout<<c-(a-b+c%a)<<endl;
        }
    }
}
